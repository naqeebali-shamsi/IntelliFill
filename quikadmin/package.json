{
  "name": "intellifill",
  "version": "1.0.0",
  "description": "IntelliFill - Intelligent document processing and form automation platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:render": "npm run build",
    "postinstall": "echo 'postinstall: sharp handled by pnpm'",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src --ignore-watch node_modules src/index.ts",
    "dev:nodemon": "nodemon src/index.ts",
    "dev:debug": "ts-node-dev --respawn --transpile-only --inspect --exit-child src/index.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/workers/queue-processor.ts",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --expose-gc\" jest --maxWorkers=1",
    "test:watch": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --expose-gc\" jest --watch --maxWorkers=1",
    "test:swarm": "node tests/swarm/run-swarm.js",
    "test:swarm:claude": "node tests/swarm/run-swarm.js --claude-flow",
    "test:api": "node tests/swarm/agents/api-tester.js",
    "test:security": "node tests/swarm/agents/security-tester.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "cli": "ts-node src/cli/index.ts",
    "test:supabase": "npx ts-node scripts/test-supabase-connection.ts",
    "test:neon-serverless": "npx ts-node scripts/test-neon-serverless.ts",
    "test:ocr-e2e": "npx ts-node tests/ocr-e2e-test.ts",
    "test:auth-e2e": "npx ts-node tests/auth-e2e-test.ts",
    "migrate:supabase": "npx ts-node scripts/migrate-users-to-supabase.ts",
    "db:seed": "npx ts-node prisma/seed.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "seed:e2e": "npx ts-node scripts/seed-e2e-users.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.952.0",
    "@aws-sdk/s3-request-presigner": "^3.952.0",
    "@google/generative-ai": "^0.24.1",
    "@neondatabase/serverless": "^1.0.2",
    "@pdf-lib/fontkit": "^1.1.1",
    "@prisma/client": "^6.14.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.12",
    "@types/pdf-parse": "^1.1.5",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.11.5",
    "commander": "^11.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^8.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "multer": "^2.0.2",
    "opossum": "^5.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.4",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.449",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "redis": "^4.6.11",
    "sharp": "^0.33.5",
    "tesseract.js": "^6.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@eslint/js": "^9.17.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.5",
    "@types/opossum": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "chalk": "^5.6.0",
    "cross-env": "^7.0.3",
    "detect-secrets": "^1.0.6",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^6.14.0",
    "puppeteer": "^24.16.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT"
}
